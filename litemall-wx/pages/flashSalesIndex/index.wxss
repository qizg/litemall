/*  加载更多   */
.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
  height: 100rpx;
  text-align: center;
  padding: 30rpx;
}

/* 返回顶部 */
.widget-goTop {
  position: fixed;
  bottom: 50rpx;
  right: 30rpx;
  background: rgba(0,0,0,0.48);
  border-radius: 50%;
  overflow: hidden;
  z-index: 1000;
}
.widget-goTop .gotop-wrap {
  display:flex;
  flex-direction:column;
  align-items:center;
  justify-content:center;
  width: 100rpx;
  height: 100rpx;
  font-size: 12px;
  color: #fff;
  border-radius: 50%;
  background-color: rgba(0,0,0,0.08);
  -webkit-background-size: 50px auto;
}
.widget-goTop .gotop-wrap .icon-top {
  width: 45rpx;
  height: 45rpx;
}

